/**app.wxss**/

page {
    font-family:PingFang-SC-Medium;
    color: #2B2B2B;
}

.flex_column {
    display: flex;
    flex-direction: column;
}

.flex_row {
    display: flex;
    flex-direction: row;
}

.share-warp{
    display: inline-block;
    position: relative;
}
.share-img{
    width:97rpx;
    height:32rpx;
}

.share-center{
    align-items: center;
}

.share-btn{
    padding: 0;
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    opacity: 0;
}

.del{
    margin-left: 10rpx;
    text-decoration: line-through;
}

.nodata{
    font-size:32rpx;
text-align:center;
margin-top:100rpx;
color:#2b2b2b;

}

.xieyi-color{
    color: #63AFE0;
}


.chatBtn{
    position: fixed;
    right: 30rpx;
    bottom: 80rpx;
}

